{
  "name": "mega",
  "version": "0.7.1",
  "description": "Unofficial Node.js SDK for mega.nz",
  "main": "dist/main.cjs.js",
  "module": "dist/main.es.js",
  "browser": "dist/main.browser.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tonistiigi/mega.git"
  },
  "keywords": [
    "mega",
    "storage"
  ],
  "scripts": {
    "build": "node build",
    "lint": "standard",
    "pretest": "node test/pre-test",
    "test": "ava || npm run posttest",
    "posttest": "node test/post-test",
    "dist": "npm run lint && npm run build -- --minify && npm run test"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
